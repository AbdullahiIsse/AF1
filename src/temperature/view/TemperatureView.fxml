<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="420.0" prefWidth="587.0" spacing="10.0" userData="Temperature presenter" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="temperature.view.TemperatureViewController">
   <children>
      <HBox>
         <children>
            <Label alignment="CENTER" prefHeight="35.0" prefWidth="597.0" text="Temperature presenter" textAlignment="CENTER">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="87.0" prefWidth="471.0" spacing="20.0">
         <children>
            <Label fx:id="filterLabel" alignment="CENTER_RIGHT" prefHeight="39.0" prefWidth="89.0" text="All" textAlignment="RIGHT">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="filterField" prefHeight="39.0" prefWidth="84.0">
               <font>
                  <Font size="18.0" />
               </font>
            </TextField>
            <Button fx:id="filterButton" mnemonicParsing="false" onAction="#onFilter" text="Filter">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </HBox>
      <AnchorPane prefHeight="200.0" prefWidth="200.0">
         <children>
            <Label fx:id="t0" layoutX="9.0" layoutY="16.0" prefHeight="27.0" prefWidth="233.0" text="t0" />
            <Label fx:id="t1" layoutX="8.0" layoutY="61.0" prefHeight="27.0" prefWidth="233.0" text="t1" />
            <Label fx:id="outputLabel" alignment="CENTER" layoutX="4.0" layoutY="101.0" prefHeight="35.0" prefWidth="233.0" text="t3" />
            <Label fx:id="radi" layoutX="500.0" layoutY="35.0" text="Radi" />
            <Button layoutX="485.0" layoutY="75.0" mnemonicParsing="false" onAction="#tuptup" text="Turn Up" />
            <Button layoutX="485.0" layoutY="123.0" mnemonicParsing="false" onAction="#tdowntdown" text="Turn Down" />
         </children>
      </AnchorPane>
      <Button mnemonicParsing="false" onAction="#nextpage" text="NextPage" />
   </children>
</VBox>
